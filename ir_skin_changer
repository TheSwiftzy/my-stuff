-- BLACK SKIN

BlackSkin:NewButton("Black Skin", "Permanent Black Skin Effect", function()
    local ohTable1 = {
    ["Skin Color"] = BrickColor.new("Really black")
}

game:GetService("ReplicatedStorage").Remote_Functions.Edit_Character:InvokeServer(ohTable1)
end)
